  end
end 

 local ammount = {}
 for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.HUD.Lost.Jaeza.ScrollingFrame:GetChildren()) do 
    if v:IsA("ImageLabel") and v:FindFirstChild("Value") then
    table.insert(items,v.Value.Text)
 end
end 

  getgenv().b = true 
