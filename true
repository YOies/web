local url = "https://discord.com/api/webhooks/1232603329608286292/IhHFPu6HgcYuEVHxUSFiTw0hf82HATz5YzSJJFiAtq8wtRGnXg0fI4ZeRINkGlSsaz-Z"
local embed = {
